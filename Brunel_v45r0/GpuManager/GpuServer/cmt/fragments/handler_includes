# start of handler_includes


$(CONSTITUENT) :: ../src/Handlers/instances.h ../src/Handlers/includes.h $(code_gen)

../src/Handlers/instances.h ::
	python $(code_gen) inst $(gpu_handlers) > $@

../src/Handlers/includes.h ::
	python $(code_gen) incl $(gpu_handlers) > $@


$(CONSTITUENT)clean ::
	$(cleanup_echo) ../src/Handlers/instances.h ../src/Handlers/includes.h
	$(cleanup_silent) /bin/rm -f ../src/Handlers/instances.h ../src/Handlers/includes.h


# end of handler_includes
