#-- start of cuda_library -----------------

$($(CONSTITUENT)_output)$(NAME).o :: $(FULLNAME)
	$(cuda_echo) $^
	$(cuda_bin) --gpu-architecture $(cuda_arch) --compiler-bindir $(cpp) --compiler-options '$(includes) -fPIC -fvisibility=hidden' --output-file $@ --compile $^

#-- end of cuda_library ------------------
