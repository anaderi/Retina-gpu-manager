namespace AST
{
	public variant Header
	{
	| Include   { path : string }
	| Namespace { id : string; lang : string }

		public override ToString() : string
		{
			match (this)
			{
			| Include(path)       => $"include \"$path\""
			| Namespace(id, lang) => $"namespace $lang $id"
			}
		}
	}
}
